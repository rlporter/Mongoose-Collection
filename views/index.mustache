<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Rock Collection</title>
  </head>
  <body>
    <h1>My Rocks</h1>
    <table>
      <thead>
        <th>Color</th>
        <th>Shape</th>
        <th>Size</th>
        <th>Type</th>
        <th>Location Found</th>
        <th>Found Date</th>
      </thead>
    </table>
    {{#rocks}}
    <tr>
      <td>
        {{.}}
      </td>
    </tr>
    {{/rocks}}

    <script src="../app.js"></script>
  </body>
</html>
